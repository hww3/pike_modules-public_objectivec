#include "../piobjc.h"

MIXIN NSArray
{

  	PIKEFUN object _get_iterator()
  	{
		push_text("Public.ObjectiveC.NSArrayIterator");
		SAFE_APPLY_MASTER("resolv", 1);

		if(Pike_sp[-1].type == T_PROGRAM)
		{
			push_object(Pike_fp->current_object);
			apply_svalue(Pike_sp-2, 1);
			stack_swap();
			pop_stack();
			return;
		}
		else
		{
			push_int(0);
		}

	}

}