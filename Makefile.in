@make_variables@
VPATH=@srcdir@:@srcdir@/../..:../..
OBJS=ObjC.o dynamic_class.o util.o OC_Array.o OC_Mapping.o AppKit.o PiObjCObject.o objc_inject.o mach_override.o method_dispatch.o libffi/src/types.o libffi/src/prep_cif.o libffi/src/powerpc/ffi_darwin.o libffi/src/powerpc/darwin_closure.o libffi/src/powerpc/darwin.o _mixins.o 
MODDIR=Public.pmod/
MODNAME=ObjectiveC
MODULE_PMOD_IN=module.pmod.in
AUTODOC_SRC_IN=objectivec.cmod
MODULE_LDFLAGS=@LDFLAGS@ @LIBS@ -framework AddressBook -framework AppKit
MODULE_CPPFLAGS=-Ilibffi -Ilibffi/include
MODULE_CFLAGS=-fobjc-exceptions
CONFIG_HEADERS=@CONFIG_HEADERS@
MODULE_INSTALL=foobar

override:  compile0
        @:

foobar:
	echo "hey!"

@dynamic_module_makefile@

compile0:
	$(RUNPIKE) $(SRCDIR)/build_mixin.pike $(SRCDIR)/mixins > _mixins.m
	$(MAKE) all
	for mod in frameworks/*; do test "frameworks/CVS" != "$$mod" && cd $$mod && pike -x module && cd $(FULL_SRC_DIR); done;

@dependencies@
